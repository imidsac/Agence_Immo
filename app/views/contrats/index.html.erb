<p id="notice"><%= notice %></p>

<h1>Listing Contrats</h1>

<table>
  <thead>
  <tr>
    <th>Article</th>
    <th>Description</th>
    <th>Type contrat</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @contrats.each do |contrat| %>
      <tr>
        <td><%= contrat.article %></td>
        <td><%= contrat.description %></td>
        <td><%= contrat.type_contrat %></td>
        <td><%= link_to 'Show', contrat %></td>
        <td><%= link_to 'Edit', edit_contrat_path(contrat) %></td>
        <td><%= link_to 'Destroy', contrat, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contrat', new_contrat_path %>
