<%= form_for @occupation, :html => {:class => 'form-horizontal'} do |f| %>
    <div class="form-group">
      <%= f.label :client_id, :class => 'control-label' %>
      <%= f.number_field :client_id, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :building_id, :class => 'control-label' %>
      <%= f.number_field :building_id, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :chamber_id, :class => 'control-label' %>
      <%= f.number_field :chamber_id, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :modalitepaiement_id, :class => 'control-label' %>
      <%= f.number_field :modalitepaiement_id, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :etat, :class => 'control-label' %>
      <%= f.text_field :etat, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :date_occupation, :class => 'control-label' %>
      <%= f.date_select :date_occupation, :class => 'form-control' %>
    </div>

    <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  occupations_path, :class => 'btn' %>
  </div>
<% end %>
