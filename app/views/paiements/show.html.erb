<%- model_class = Paiement -%>
<div class="page-header">
  <h1><%= t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:num) %>:</strong></dt>
  <dd><%= @paiement.num %></dd>
  <dt><strong><%= model_class.human_attribute_name(:num_paie) %>:</strong></dt>
  <dd><%= @paiement.num_paie %></dd>
  <dt><strong><%= model_class.human_attribute_name(:owner_id) %>:</strong></dt>
  <dd><%= @paiement.owner_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:client_id) %>:</strong></dt>
  <dd><%= @paiement.client_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:banque_id) %>:</strong></dt>
  <dd><%= @paiement.banque_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:condition_reg) %>:</strong></dt>
  <dd><%= @paiement.condition_reg %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type_paiement) %>:</strong></dt>
  <dd><%= @paiement.type_paiement %></dd>
  <dt><strong><%= model_class.human_attribute_name(:motif) %>:</strong></dt>
  <dd><%= @paiement.motif %></dd>
  <dt><strong><%= model_class.human_attribute_name(:debit) %>:</strong></dt>
  <dd><%= @paiement.debit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:credit) %>:</strong></dt>
  <dd><%= @paiement.credit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:datepaiement) %>:</strong></dt>
  <dd><%= @paiement.datepaiement %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              paiements_path, :class => 'btn' %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_paiement_path(@paiement), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              paiement_path(@paiement),
              :method => 'delete',
              :data => {:confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))},
              :class => 'btn btn-danger' %>
</div>
