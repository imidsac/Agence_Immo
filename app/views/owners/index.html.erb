<p id="notice"><%= notice %></p>

<h1>Listing Owners</h1>

<table>
  <thead>
    <tr>
      <th>Num</th>
      <th>Codeowner</th>
      <th>Nom</th>
      <th>Prenom</th>
      <th>Sexe</th>
      <th>Date nai</th>
      <th>Lieu nai</th>
      <th>Phone</th>
      <th>Address</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.num %></td>
        <td><%= owner.codeowner %></td>
        <td><%= owner.nom %></td>
        <td><%= owner.prenom %></td>
        <td><%= owner.sexe %></td>
        <td><%= owner.date_nai %></td>
        <td><%= owner.lieu_nai %></td>
        <td><%= owner.phone %></td>
        <td><%= owner.address %></td>
        <td><%= owner.email %></td>
        <td><%= link_to 'Show', owner %></td>
        <td><%= link_to 'Edit', edit_owner_path(owner) %></td>
        <td><%= link_to 'Destroy', owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owner', new_owner_path %>
