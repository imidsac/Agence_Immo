<%- model_class = Owner -%>
<div class="page-header">
  <h1><%= t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:num) %></th>
      <th><%= model_class.human_attribute_name(:codeowner) %></th>
      <th><%= model_class.human_attribute_name(:nom) %></th>
      <th><%= model_class.human_attribute_name(:prenom) %></th>
      <th><%= model_class.human_attribute_name(:sexe) %></th>
      <th><%= model_class.human_attribute_name(:date_nai) %></th>
      <th><%= model_class.human_attribute_name(:lieu_nai) %></th>
      <th><%= model_class.human_attribute_name(:nationalite) %></th>
      <th><%= model_class.human_attribute_name(:profession) %></th>
      <th><%= model_class.human_attribute_name(:phone) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:type_ow) %></th>
      <th><%= model_class.human_attribute_name(:etat) %></th>
      <th><%= model_class.human_attribute_name(:date_ins) %></th>
      <th><%= model_class.human_attribute_name(:avatar_file_name) %></th>
      <th><%= model_class.human_attribute_name(:avatar_content_type) %></th>
      <th><%= model_class.human_attribute_name(:avatar_file_size) %></th>
      <th><%= model_class.human_attribute_name(:avatar_updated_at) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%= t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= link_to owner.id, owner_path(owner) %></td>
        <td><%= owner.num %></td>
        <td><%= owner.codeowner %></td>
        <td><%= owner.nom %></td>
        <td><%= owner.prenom %></td>
        <td><%= owner.sexe %></td>
        <td><%= owner.date_nai %></td>
        <td><%= owner.lieu_nai %></td>
        <td><%= owner.nationalite %></td>
        <td><%= owner.profession %></td>
        <td><%= owner.phone %></td>
        <td><%= owner.address %></td>
        <td><%= owner.email %></td>
        <td><%= owner.type_ow %></td>
        <td><%= owner.etat %></td>
        <td><%= owner.date_ins %></td>
        <td><%= owner.avatar_file_name %></td>
        <td><%= owner.avatar_content_type %></td>
        <td><%= owner.avatar_file_size %></td>
        <td><%= owner.avatar_updated_at %></td>
        <td><%= l owner.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_owner_path(owner), :class => 'btn btn-primary btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      owner_path(owner),
                      :method => :delete,
                      :data => {:confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))},
                      :class => 'btn btn-danger btn-xs' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_owner_path,
            :class => 'btn btn-primary' %>
