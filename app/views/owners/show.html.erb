<%- model_class = Owner -%>
<div class="page-header">
  <h1><%= t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:num) %>:</strong></dt>
  <dd><%= @owner.num %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codeowner) %>:</strong></dt>
  <dd><%= @owner.codeowner %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nom) %>:</strong></dt>
  <dd><%= @owner.nom %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prenom) %>:</strong></dt>
  <dd><%= @owner.prenom %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sexe) %>:</strong></dt>
  <dd><%= @owner.sexe %></dd>
  <dt><strong><%= model_class.human_attribute_name(:date_nai) %>:</strong></dt>
  <dd><%= @owner.date_nai %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lieu_nai) %>:</strong></dt>
  <dd><%= @owner.lieu_nai %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nationalite) %>:</strong></dt>
  <dd><%= @owner.nationalite %></dd>
  <dt><strong><%= model_class.human_attribute_name(:profession) %>:</strong></dt>
  <dd><%= @owner.profession %></dd>
  <dt><strong><%= model_class.human_attribute_name(:phone) %>:</strong></dt>
  <dd><%= @owner.phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @owner.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @owner.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type_ow) %>:</strong></dt>
  <dd><%= @owner.type_ow %></dd>
  <dt><strong><%= model_class.human_attribute_name(:etat) %>:</strong></dt>
  <dd><%= @owner.etat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:date_ins) %>:</strong></dt>
  <dd><%= @owner.date_ins %></dd>
  <dt><strong><%= model_class.human_attribute_name(:avatar_file_name) %>:</strong></dt>
  <dd><%= @owner.avatar_file_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:avatar_content_type) %>:</strong></dt>
  <dd><%= @owner.avatar_content_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:avatar_file_size) %>:</strong></dt>
  <dd><%= @owner.avatar_file_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:avatar_updated_at) %>:</strong></dt>
  <dd><%= @owner.avatar_updated_at %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              owners_path, :class => 'btn' %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_owner_path(@owner), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              owner_path(@owner),
              :method => 'delete',
              :data => {:confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))},
              :class => 'btn btn-danger' %>
</div>
