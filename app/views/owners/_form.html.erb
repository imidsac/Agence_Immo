<%= form_for @owner, :html => {:class => 'form-horizontal'} do |f| %>
    <div class="form-group">
      <%= f.label :num, :class => 'control-label' %>
      <%= f.text_field :num, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :codeowner, :class => 'control-label' %>
      <%= f.text_field :codeowner, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :nom, :class => 'control-label' %>
      <%= f.text_field :nom, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :prenom, :class => 'control-label' %>
      <%= f.text_field :prenom, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :sexe, :class => 'control-label' %>
      <%= f.text_field :sexe, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :date_nai, :class => 'control-label' %>
      <%= f.date_select :date_nai, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :lieu_nai, :class => 'control-label' %>
      <%= f.text_field :lieu_nai, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :nationalite, :class => 'control-label' %>
      <%= f.text_field :nationalite, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :profession, :class => 'control-label' %>
      <%= f.text_field :profession, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :phone, :class => 'control-label' %>
      <%= f.text_field :phone, :class => 'form-control' %>
  </div>
    <div class="form-group">
      <%= f.label :address, :class => 'control-label' %>
      <%= f.text_area :address, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :email, :class => 'control-label' %>
      <%= f.text_field :email, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :type_ow, :class => 'control-label' %>
      <%= f.text_field :type_ow, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :etat, :class => 'control-label' %>
      <%= f.text_field :etat, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :date_ins, :class => 'control-label' %>
      <%= f.date_select :date_ins, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :avatar_file_name, :class => 'control-label' %>
      <%= f.text_field :avatar_file_name, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :avatar_content_type, :class => 'control-label' %>
      <%= f.text_field :avatar_content_type, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :avatar_file_size, :class => 'control-label' %>
      <%= f.number_field :avatar_file_size, :class => 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :avatar_updated_at, :class => 'control-label' %>
      <%= f.datetime_select :avatar_updated_at, :class => 'form-control' %>
    </div>

    <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  owners_path, :class => 'btn' %>
  </div>
<% end %>
