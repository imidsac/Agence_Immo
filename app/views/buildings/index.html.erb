<%- model_class = Building -%>
<div class="page-header">
  <h1><%= t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:owner_id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:type_build) %></th>
      <th><%= model_class.human_attribute_name(:niveau) %></th>
      <th><%= model_class.human_attribute_name(:door) %></th>
      <th><%= model_class.human_attribute_name(:street) %></th>
      <th><%= model_class.human_attribute_name(:quartier) %></th>
      <th><%= model_class.human_attribute_name(:ville) %></th>
      <th><%= model_class.human_attribute_name(:unique) %></th>
      <th><%= model_class.human_attribute_name(:previen) %></th>
      <th><%= model_class.human_attribute_name(:pordinaire) %></th>
      <th><%= model_class.human_attribute_name(:pong) %></th>
      <th><%= model_class.human_attribute_name(:pgov) %></th>
      <th><%= model_class.human_attribute_name(:pourcent_pordinaire) %></th>
      <th><%= model_class.human_attribute_name(:pourcent_pong) %></th>
      <th><%= model_class.human_attribute_name(:pourcent_pgov) %></th>
      <th><%= model_class.human_attribute_name(:prix) %></th>
      <th><%= model_class.human_attribute_name(:etat) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%= t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @buildings.each do |building| %>
      <tr>
        <td><%= link_to building.id, building_path(building) %></td>
        <td><%= building.owner_id %></td>
        <td><%= building.name %></td>
        <td><%= building.type_build %></td>
        <td><%= building.niveau %></td>
        <td><%= building.door %></td>
        <td><%= building.street %></td>
        <td><%= building.quartier %></td>
        <td><%= building.ville %></td>
        <td><%= building.unique %></td>
        <td><%= building.previen %></td>
        <td><%= building.pordinaire %></td>
        <td><%= building.pong %></td>
        <td><%= building.pgov %></td>
        <td><%= building.pourcent_pordinaire %></td>
        <td><%= building.pourcent_pong %></td>
        <td><%= building.pourcent_pgov %></td>
        <td><%= building.prix %></td>
        <td><%= building.etat %></td>
        <td><%= l building.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_building_path(building), :class => 'btn btn-primary btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      building_path(building),
                      :method => :delete,
                      :data => {:confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))},
                      :class => 'btn btn-danger btn-xs' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_building_path,
            :class => 'btn btn-primary' %>
